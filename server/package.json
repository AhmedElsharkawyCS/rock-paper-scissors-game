{
  "name": "rock-paper-scissors",
  "version": "1.0.0",
  "description": "Rock paper scissors is a hand game, usually played between two people, in which each player simultaneously forms one of three shapes with an outstretched hand. These shapes are [rock, paper, scissors]",
  "main": "build/index.js",
  "scripts": {
    "start:cli": "ts-node-dev  src/cli.ts",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "watch": "tsc -w",
    "build": "tsc",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 30000,
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "keywords": [
    "Node",
    "Typescript",
    "javascript",
    "express",
    "jest"
  ],
  "homepage": "https://github.com/AhmedElsharkawyCS/advanced-microservices-ticketing-app",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/prompts": "^2.0.14",
    "@types/supertest": "^2.0.11",
    "jest": "^27.3.1",
    "socket.io-client": "^4.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "prompts": "^2.4.2",
    "socket.io": "^4.4.1",
    "ts-node-dev": "^1.1.8",
    "uid": "^2.0.0"
  }
}
